<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/open_shop_parent"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="car.tzxb.b2b.Uis.OpenShopPackage.OpenShopActivity">
  <include
      android:id="@+id/open_shop_actionbar"
      layout="@layout/base_red_actionbar"/>
    <android.support.v4.widget.NestedScrollView
        android:layout_below="@+id/open_shop_actionbar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <include layout="@layout/open_shop_header"/>
            <include layout="@layout/open_shop_footer"/>

        </LinearLayout>
    </android.support.v4.widget.NestedScrollView>
</RelativeLayout>
