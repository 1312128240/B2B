<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="car.tzxb.b2b.Uis.OpenShopPackage.OpenShopMapActivity">
  <include
      android:id="@+id/open_shop_map_actionabr"
      layout="@layout/base_red_actionbar"/>
    <com.baidu.mapapi.map.MapView
        android:id="@+id/open_shop_mapview"
        android:layout_below="@+id/open_shop_map_actionabr"
        android:layout_above="@+id/btn_sumbit_open_shop_latlog"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

    </com.baidu.mapapi.map.MapView>
  <TextView
      android:id="@+id/tv_open_shop_latlog"
      android:text="深圳观澜湖"
      android:gravity="center"
      android:padding="15dp"
      android:layout_marginLeft="20dp"
      android:layout_marginRight="20dp"
      android:layout_above="@+id/btn_sumbit_open_shop_latlog"
      android:layout_marginBottom="20dp"
      android:background="@drawable/bg2"
      android:layout_width="match_parent"
      android:layout_height="wrap_content" />
    <Button
        android:id="@+id/btn_sumbit_open_shop_latlog"
        android:layout_alignParentBottom="true"
        android:background="@color/colorWhite"
        android:text="确认"
        android:layout_width="match_parent"
        android:layout_height="45dp" />
</RelativeLayout>
